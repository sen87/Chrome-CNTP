{
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "content_security_policy": "script-src 'self' https://query.yahooapis.com; object-src 'self'",
   "description": "Override the new tab page with something more shiny.",
   "icons": {
      "128": "favicon/icon128.png",
      "16": "favicon/icon16.png",
      "32": "favicon/icon32.png",
      "48": "favicon/icon48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "CNTP - Custom New Tab Page",
   "options_page": "options.html",
   "options_ui": {
       "chrome_style": true,
       "open_in_tab": true,
       "page": "options.html"
    },
   "permissions": [ "storage", "chrome://favicon/", "http://*/", "https://*/" ],
   "version": "1.1"
}
